@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <https://raw.githubusercontent.com/KEGP/testing/main/artist/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ex:artistcq_alias.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "By which aliases is the Artist known?" ;
 	owlunit:hasSPARQLUnitTest "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX : <https://www.unibo.it/ontologies/metadata/> SELECT DISTINCT ?aliasname WHERE {?artist :hasAlias ?alias . ?artist :hasMusicBrainzID \"e651adec-e027-42ee-89fa-9970c6f60fe7\". ?alias :hasName ?aliasname}" ;
	owlunit:hasInputData ex:artistandrelationdata.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
  owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"aliasname\" ] } ,  \"results\": {  \"bindings\": [ {  \"aliasname\": [ {  \"type\":  \"literal\" ,  \"value\":  \"The Gene Krupa Orchestra\" }, {  \"type\":  \"literal\" ,  \"value\":  \"Gene Krupa & His Orchestra\" } ] } ] } }";
  owlunit:testsOntology rdf: .
