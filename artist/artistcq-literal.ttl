@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <https://raw.githubusercontent.com/KEGP/testing/main/artist/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ex:artistcq-literal.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "What is the name of the Artist?" ;
 	owlunit:hasSPARQLUnitTest "PREFIX our: <https://www.unibo.it/ontologies/metadata/> SELECT DISTINCT ?name WHERE {?artist our:hasName ?name}" ;
	owlunit:hasInputData ex:artistdata.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
  owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"name\" ] } ,  \"results\": {  \"bindings\": [ {  \"name\": {  \"type\":  \"literal\" ,  \"value\":  \"The Benny Goodman Sextet\" } } ] } }";
  owlunit:testsOntology our: .
