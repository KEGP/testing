@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <https://raw.githubusercontent.com/KEGP/testing/main/artist/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix our: <https://www.unibo.it/ontologies/metadata/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ex:artistcq.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "What is the name of the Artist?" ;
 	owlunit:hasSPARQLUnitTest "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> SELECT DISTINCT ?type WHERE {?artist rdf:type ?type}" ;
	owlunit:hasInputData ex:artistdata.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
  owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"name\" ] } ,  \"results\": {  \"bindings\": [ {  \"name\": {  \"type\":  \"uri\" ,  \"value\":  \"https://www.unibo.it/ontologies/metadata/Band\" } } ] } }".
